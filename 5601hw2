disp("3.2-11")
3.2-11
for n= 10.^[2:2:8]
v1= sin([1:n])./[1:n];
v2= 1./[1:n].^2;
v3= 1./[1:n].^4;
disp("n = e"+ log10(n) + "  sum1 "+"sum2 "+"sum3")
disp([sum(v1) sum(v2) sum(v3)])
end
n = e2  sum1 sum2 sum3
